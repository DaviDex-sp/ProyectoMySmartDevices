@page
@model ProyectoMSD.Pages.Almacenan.EditModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Almacenan</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Almacenan.Id" />
            <div class="form-group form-check">
                
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Almacenan.Estado" /> @Html.DisplayNameFor(model => model.Almacenan.Estado)
                </label>
            </div>
            <div class="form-group">
                
                <label asp-for="Almacenan.IdEspacios" class="control-label"></label>
                <select asp-for="Almacenan.IdEspacios" class="form-control" asp-items="ViewBag.IdEspacios"></select>
                <span asp-validation-for="Almacenan.IdEspacios" class="text-danger"></span>
            </div>
            <div class="form-group">
                
                <label asp-for="Almacenan.IdDispositivos" class="control-label"></label>
                <select asp-for="Almacenan.IdDispositivos" class="form-control" asp-items="ViewBag.IdDispositivos"></select>
                <span asp-validation-for="Almacenan.IdDispositivos" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
