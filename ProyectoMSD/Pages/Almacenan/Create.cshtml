            </div>
            <div class="form-group">
                <label asp-for="Almacenan.IdDispositivos" class="control-label"></label>
                <select asp-for="Almacenan.IdDispositivos" class ="form-control" asp-items="ViewBag.IdDispositivos"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
