@page
@model ProyectoMSD.Pages.Almacenan.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Almacenan[0].Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Almacenan[0].IdDispositivosNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Almacenan[0].IdEspaciosNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Almacenan) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDispositivosNavigation.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEspaciosNavigation.Id)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
